{
  "name": "Security-Tools",
  "version": "1.0.0",
  "description": "Security-Tools",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . Security-Tools",
    "build-linux32": "electron-packager . Security-Tools --platform=linux  --arch=ia32",
    "build-linux64": "electron-packager . Security-Tools --platform=linux  --arch=x64",
    "build-mac": "electron-packager . Security-Tools --platform=darwin  --arch=x64",
    "build-win32": "electron-packager . Security-Tools --platform=win32  --arch=ia32",
    "build-win64": "electron-packager . Security-Tools --platform=win32  --arch=x64"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^19.48.2",
    "electron-packager": "^10.1.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.3"
  }
}
